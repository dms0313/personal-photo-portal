# Environment Variables Template
# Copy this to .env and fill in your values

# ImageKit Configuration
VITE_IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_imagekit_id
VITE_IMAGEKIT_PUBLIC_KEY=public_your_public_key
IMAGEKIT_PRIVATE_KEY=private_your_private_key

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key

# Owner Authentication
VITE_OWNER_PASSWORD=your_secure_password
VITE_OWNER_EMAIL=owner@example.com
